    "react-bootstrap": "^1.5.2",
    "react-bootstrap-table-next": "^3.3.5",
    "react-bootstrap-table2-paginator": "^2.1.0",
    "react-bootstrap-table2-toolkit": "^2.1.2",
    "react-color": "^2.19.3",
    "react-datepicker": "^3.0.0",
